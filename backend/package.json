    "@types/nodemailer": "^6.4.6",
    "@types/passport": "^1.0.11",
    "@types/xml2js": "^0.4.11",
    "@willsoto/nestjs-objection": "8.1.0",
    "ajv-formats": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "bull": "3.18.0",
    "cache-manager": "3.4.0",
    "cache-manager-redis-store": "2.0.0",
    "casbin": "5.1.3",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "dayjs": "^1.11.6",
    "express-session": "^1.17.3",
    "fix-esm": "^1.0.1",
    "form-data": "^4.0.0",
    "generate-password": "^1.7.0",
    "handlebars": "^4.7.7",
    "joi": "17.2.1",
    "knex": "2.3.0",
